{
    "name": "@unirep/contracts",
    "version": "1.0.0",
    "main": "build/src/index.js",
    "types": "build/src/index.d.ts",
    "author": "vivianjeng <vivi432@yahoo.com.tw>",
    "license": "MIT",
    "scripts": {
        "pre": "node scripts/prepare",
        "build": "npx ts-node scripts/buildVerifiers.ts && yarn compile && yarn typechain && tsc && yarn lint",
        "compile": "npx hardhat compile",
        "typechain": "npx hardhat typechain && yarn genABI && yarn removeFactory",
        "genABI": "npx ts-node scripts/abiGenerator.ts",
        "removeFactory": "npx ts-node scripts/removeFactory.ts",
        "deploy": "hardhat run --no-compile --network local scripts/deploy.js",
        "test": "npx hardhat test --no-compile",
        "lint": "prettier --write ."
    },
    "dependencies": {
        "@unirep/crypto": "1.0.0",
        "ethers": "^5.6.8"
    },
    "devDependencies": {
        "@ethersproject/abi": "^5.6.3",
        "@ethersproject/bytes": "^5.6.1",
        "@ethersproject/providers": "^5.6.8",
        "@nomiclabs/hardhat-ethers": "^2.0.6",
        "@nomiclabs/hardhat-waffle": "^2.0.3",
        "@openzeppelin/contracts": "^4.6.0",
        "@typechain/ethers-v5": "^10.1.0",
        "@typechain/hardhat": "^6.1.0",
        "ethereum-waffle": "^3.4.4",
        "hardhat": "^2.9.9",
        "hardhat-gas-reporter": "^1.0.8",
        "typechain": "^8.1.0",
        "typescript": "^4.7.3"
    }
}
