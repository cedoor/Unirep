{
    "name": "@unirep/circuits",
    "version": "1.0.0",
    "scripts": {
        "pre": "node scripts/prepare",
        "build": "yarn downloadPtau && yarn buildCircuits && yarn buildSnarks && tsc",
        "downloadPtau": "npx ts-node scripts/downloadPtau.ts",
        "buildCircuits": "npx ts-node scripts/buildCircuits.ts",
        "buildSnarks": "npx ts-node scripts/buildSnarks.ts",
        "buildTestCircuits": "npx ts-node scripts/buildTestCircuits.ts",
        "buildTs": "tsc",
        "test": "yarn buildTestCircuits && mocha -r ts-node/register test/*.test.ts --exit",
        "lint": "prettier --write ."
    },
    "repository": "https://github.com/unirep/unirep.git",
    "author": "vivianjeng <vivi432@yahoo.com.tw>",
    "license": "MIT",
    "dependencies": {
        "@unirep/crypto": "1.0.0"
    },
    "devDependencies": {
        "circom": "^0.5.46",
        "circomlib": "0.5.3",
        "mocha": "^10.0.0",
        "shelljs": "^0.8.5",
        "snarkjs": "^0.4.7"
    }
}
